{
  "name": "Tukey Test",
  "description": "Tests for significant pairwise differences among group means while controlling for Family-Wise Error Rate.",
  "tags": [
    "statistical testing",
    "linear model"
  ],
  "authors": [
    "tercen"
  ],
  "urls": [
    "https://github.com/tercen/tukey_hsd_operator"
  ],
  "container": "tercen/tukey_hsd_operator:0.0.2",
  "properties": [
    {
      "kind": "EnumeratedProperty",
      "name": "method",
      "defaultValue": "one.way",
      "values": [
        "one.way",
        "two.way"
      ],
      "description": "Whether to consider interactions between effects (two.way) or not (one.way) in the model."
    }
  ],
  "operatorSpec": {
    "kind": "OperatorSpec",
    "ontologyUri": "https://tercen.com/_ontology/tercen",
    "ontologyVersion": "0.0.1",
    "inputSpecs": [
      {
        "kind": "CrosstabSpec",
        "metaFactors": [
          {
            "kind": "MetaFactor",
            "name": "Groups",
            "type": "",
            "description": "Represents the groups to compare.",
            "ontologyMapping": "color",
            "crosstabMapping": "color",
            "cardinality": "1..n",
            "factors": []
          },
          {
            "kind": "MetaFactor",
            "name": "Random Effects",
            "type": "",
            "description": "Represents the random effects to include in the model (optional).",
            "ontologyMapping": "labels",
            "crosstabMapping": "labels",
            "cardinality": "0..n",
            "factors": []
          }
        ],
        "axis": [
          {
            "kind": "AxisSpec",
            "metaFactors": [
              {
                "kind": "MetaFactor",
                "name": "Measurement",
                "type": "numeric",
                "description": "Measurement values.",
                "ontologyMapping": "measurement",
                "crosstabMapping": "y",
                "cardinality": "1",
                "factors": []
              }
            ]
          }
        ]
      }
    ]
  }
}